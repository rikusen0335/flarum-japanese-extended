{
  "name": "rikusen0335/lang-japanese-extended",
  "description": "Japanese language pack for Flarum's third-party extensions.",
  "keywords": [
    "flarum",
    "extension",
    "japanese",
    "locale",
    "translation"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^0.1.0-beta.9"
  },
  "suggest": {
    "naga0ka/flarum-ext-japanese": "Japanese"
  },
  "support": {
    "issues": "https://github.com/rikusen0335/lang-japanese-extended/issues",
    "source": "https://github.com/rikusen0335/lang-japanese-extended/"
  },
  "authors": [
    {
      "name": "rikusen0335",
      "email": "rikusen0335@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "RIKUSEN0335\\LangJapaneseExtended\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "Japanese Extended Pack",
      "icon": {
        "name": "fas fa-circle",
        "background": "#ffffff",
        "backgroundSize": "cover",
        "backgroundPosition": "center",
        "color": "#FF0000"
      }
    },
    "flarum-locale": {
      "code": "ja",
      "title": "Japanese"
    }
  }
}
